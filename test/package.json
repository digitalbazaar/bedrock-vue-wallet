{
  "name": "bedrock-vue-wallet-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "scripts": {
    "debug": "node --preserve-symlinks test.js test --framework karma --log-level debug",
    "test": "node --preserve-symlinks test.js test --framework karma",
    "coverage": "cross-env NODE_ENV=test c8 npm run test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm run test"
  },
  "dependencies": {
    "@bedrock/core": "^6.0.0",
    "@bedrock/express": "^8.0.0",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/karma": "^6.0.0",
    "@bedrock/mongodb": "^10.0.0",
    "@bedrock/quasar": "^8.0.1",
    "@bedrock/server": "^5.0.0",
    "@bedrock/test": "^8.0.0",
    "@bedrock/vue": "^4.0.0",
    "@bedrock/vue-credential-card": "^5.0.0",
    "@bedrock/vue-wallet": "file:..",
    "@bedrock/web": "^2.0.1",
    "@bedrock/web-account": "^3.0.2",
    "@bedrock/web-fontawesome": "^2.0.0",
    "@bedrock/web-pouch-edv": "^5.0.0",
    "@bedrock/web-profile-manager": "^17.0.0",
    "@bedrock/web-session": "^4.0.1",
    "@bedrock/web-vc-store": "^7.0.0",
    "@bedrock/web-wallet": "^7.0.0",
    "@digitalbazaar/vue-extendable-event": "^4.1.0",
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "@vueuse/core": "^8.5.0",
    "ajv": "^6.12.6",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/vue-wallet/**"
    ],
    "exclude": [
      "node_modules/@bedrock/vue-wallet/node_modules/**"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  }
}
